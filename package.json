{
  "name": "otto-assistant",
  "version": "2.0.0",
  "description": "AI-powered live audio processing with real-time board updates for creative workflows",
  "main": "src/index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "bin": {
    "otto-live": "./bin/otto-live",
    "otto-debug": "./bin/otto-debug",
    "otto-export": "./bin/otto-export"
  },
  "scripts": {
    "start": "node live-mode-simple.js",
    "live": "node bin/otto-live",
    "live-simple": "node live-mode-simple.js",
    "debug": "node bin/otto-debug",
    "export": "node bin/otto-export",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest src/**/*.test.js",
    "test:integration": "jest tests/integration/**/*.test.js",
    "test:audio": "node bin/otto-debug --audio",
    "test:api": "node bin/otto-debug --api",
    "test:system": "node bin/otto-debug --quick",
    "simulation": "node bin/otto-live --simulation",
    "dev": "nodemon bin/otto-live --simulation",
    "dev:debug": "nodemon bin/otto-live --simulation --debug",
    "setup": "node scripts/setup.js",
    "postinstall": "node scripts/post-install.js",
    "build": "npm run build:docs && npm run build:dist",
    "build:docs": "jsdoc src -r -d docs --readme README.md",
    "build:dist": "pkg package.json --out-path dist/",
    "lint": "eslint src/ bin/ --ext .js",
    "lint:fix": "eslint src/ bin/ --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\" \"bin/*\" \"tests/**/*.js\"",
    "validate": "npm run lint && npm run test && npm run test:system",
    "clean": "rimraf temp-* exports/* dist/* docs/*",
    "docker:build": "docker build -t otto-assistant .",
    "docker:run": "docker run -it --rm -v $(pwd)/exports:/app/exports otto-assistant",
    "release": "semantic-release",
    "precommit": "lint-staged",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "outdated": "npm outdated",
    "update:deps": "ncu -u && npm install"
  },
  "keywords": [
    "ai",
    "audio-processing",
    "live-transcription",
    "real-time",
    "miro",
    "notion",
    "obsidian",
    "creative-workflow",
    "voice-recognition",
    "meeting-assistant",
    "automation",
    "productivity"
  ],
  "author": {
    "name": "Otto Assistant Team",
    "email": "team@otto-assistant.com",
    "url": "https://otto-assistant.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/otto-assistant.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/otto-assistant/issues"
  },
  "homepage": "https://github.com/your-org/otto-assistant#readme",
  "dependencies": {
    "axios": "^1.9.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.6.0",
    "commander": "^12.0.0",
    "dotenv": "^16.5.0",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "inquirer": "^8.2.6",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "node-cron": "^3.0.3",
    "ora": "^5.4.1",
    "ws": "^8.17.0",
    "yaml": "^2.4.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.3",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "npm-check-updates": "^16.14.20",
    "pkg": "^5.8.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.6",
    "semantic-release": "^23.0.8",
    "supertest": "^7.0.0"
  },
  "optionalDependencies": {
    "ffmpeg-static": "^5.2.0",
    "sharp": "^0.33.3"
  },
  "peerDependencies": {
    "node-gyp": ">=9.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "files": [
    "bin/",
    "src/",
    "scripts/",
    "templates/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{md,json,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run validate"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/temp/**"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "prettier"
    ],
    "env": {
      "node": true,
      "es2021": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "node/no-unpublished-require": "off",
      "node/no-missing-require": "off"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false
  },
  "release": {
    "branches": [
      "main",
      "next"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/otto-assistant"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
